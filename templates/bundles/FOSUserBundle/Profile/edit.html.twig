{% extends "@FOSUser/layout.html.twig" %}

{% block fos_user_content %}
    <div class="card text-dark">
        <div class="card-header">
            <h1 class="text-center">Profil</h1>
        </div>
        <div class="card-body">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button class="btn btn-happy float-right">{{ 'form.submit' | trans }}</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
